{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SalesConnection": "Server=(localdb)\\mssqllocaldb;Database=LSalesPro_Sales;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "ServiceSettings": {
    "InventoryService": {
      "Url": "https://localhost:5003",
      "Timeout": 30
    },
    "AuthService": {
      "Url": "https://localhost:5001",
      "Timeout": 30
    }
  },
  "JwtSettings": {
    "Issuer": "LSalesPro.AuthService",
    "Audience": "LSalesPro.ClientApps",
    "SecretKey": "DefaultSecretKeyForDevelopmentOnlyDoNotUseInProduction",
    "TokenExpirationMinutes": 120
  }
}
